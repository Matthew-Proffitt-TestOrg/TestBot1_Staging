{
  "name": "@org/cli",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "bin": {
    "org-cli": "dist/index.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "typecheck": "tsc -p tsconfig.typecheck.json --noEmit",
    "lint": "pnpm run typecheck"
  },
  "dependencies": {
    "@org/types": "workspace:*",
    "@org/wallet": "workspace:*",
    "chalk": "^5.3.0",
    "commander": "^11.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.2"
  }
}
